{
  "name": "web-template",
  "version": "1.0.0",
  "description": "web development.",
  "repository": {
    "type": "git",
    "url": "git@github.com:kajiyan/web-boilerplate.git"
  },
  "author": "Hiroki Kajiya",
  "browserslist": [
    ">0.25%",
    "ie 11",
    "not op_mini all",
    "not Opera 55"
  ],
  "engines": {
    "npm": ">=6",
    "node": ">=10"
  },
  "license": "MIT",
  "private": true,
  "scripts": {
    "local:dev:server": "cross-env NODE_ENV=development node server",
    "local:dev:template": "cross-env NODE_ENV=development node internals/pug",
    "global:dev:template": "cross-env NODE_ENV=development node internals/pug --location=global",
    "global:pro:template": "cross-env NODE_ENV=production node internals/pug",
    "global:dev:scripts": "cross-env NODE_ENV=development node internals/webpack --location=global",
    "global:pro:scripts": "cross-env NODE_ENV=production node internals/webpack",
    "local:dev:styles": "cross-env NODE_ENV=development node internals/postcss",
    "global:dev:styles": "cross-env NODE_ENV=development node internals/postcss --location=global",
    "global:pro:styles": "cross-env NODE_ENV=production node internals/postcss",
    "rm:dev": "rimraf development-htdocs",
    "rm:pro": "rimraf production-htdocs",
    "mkdirp:dev": "mkdirp development-htdocs/assets",
    "mkdirp:pro": "mkdirp production-htdocs/assets",
    "cpx:dev": "cpx \"htdocs/**/**/*.{js,gif,png,jpg,jpeg,ico,svg,mp3,mp4,webm,eot,ttf,otf,woff,woff2,xml,json,txt,webmanifest,htaccess}\" development-htdocs",
    "cpx:pro": "cpx \"htdocs/**/**/*.{js,gif,png,jpg,jpeg,ico,svg,mp3,mp4,webm,eot,ttf,otf,woff,woff2,xml,json,txt,webmanifest,htaccess}\" production-htdocs",
    "lint:styles": "stylelint \"**/*.pcss\"",
    "lint:styles:fix": "stylelint --fix",
    "lint:scripts": "eslint \"app\" --ext .js,.jsx --config .eslintrc.js --ignore-path .gitignore --ignore-pattern internals",
    "lint:scripts:fix": "eslint \"app\" --ext .js,.jsx --config .eslintrc.js --ignore-path .gitignore --ignore-pattern internals --fix",
    "lint:ts": "tslint --config tslint.json --project tsconfig.json",
    "lint:ts:fix": "tslint --config tslint.json --project tsconfig.json --fix",
    "build": "cross-env NODE_ENV=production run-s rm:pro mkdirp:pro cpx:pro global:pro:template global:pro:scripts",
    "build-dev": "cross-env NODE_ENV=development run-s rm:dev mkdirp:dev cpx:dev global:dev:template global:dev:scripts",
    "dev": "cross-env NODE_ENV=development run-p local:dev:server local:dev:template",
    "watch-build": "cross-env NODE_ENV=production run-s rm:pro mkdirp:pro cpx:pro global:pro:template global:pro:styles global:pro:scripts",
    "watch-build-dev": "cross-env NODE_ENV=development run-s rm:dev mkdirp:dev cpx:dev global:dev:template global:dev:styles global:dev:scripts",
    "watch": "cross-env NODE_ENV=development run-p local:dev:server local:dev:template local:dev:styles"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.pcss": [
      "yarn run lint:styles:fix",
      "git add --force"
    ],
    "*.{js,jsx}": [
      "yarn run lint:scripts:fix",
      "git add --force"
    ],
    "*.{ts,tsx}": [
      "yarn run lint:ts:fix",
      "git add --force"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@babel/preset-env": "^7.3.1",
    "@babel/preset-react": "^7.0.0",
    "@babel/register": "^7.0.0",
    "@types/history": "^4.7.2",
    "@types/react": "^16.8.1",
    "@types/react-dom": "^16.0.11",
    "@types/react-router": "^4.4.3",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.5",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "browser-sync": "^2.26.3",
    "browserslist": "^4.4.1",
    "case-sensitive-paths-webpack-plugin": "^2.2.0",
    "chalk": "^2.4.2",
    "chokidar": "^2.0.4",
    "circular-dependency-plugin": "^5.0.2",
    "config": "^3.0.1",
    "cpx": "^1.5.0",
    "cross-env": "^5.2.0",
    "css-loader": "^2.1.0",
    "css-mqpacker": "^7.0.0",
    "cssnano": "^4.1.8",
    "eslint": "^5.12.1",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^4.0.0",
    "eslint-loader": "^2.1.1",
    "eslint-plugin-compat": "^2.6.3",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-jsx-a11y": "^6.2.0",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-react": "^7.12.4",
    "fork-ts-checker-webpack-plugin": "^0.5.2",
    "glob": "^7.1.3",
    "husky": "^1.3.1",
    "json-loader": "^0.5.7",
    "lint-staged": "^8.1.3",
    "mkdirp": "^0.5.1",
    "modernizr-loader": "^1.0.1",
    "moment": "^2.24.0",
    "npm-run-all": "^4.1.5",
    "pixrem": "^5.0.0",
    "postcss": "^7.0.14",
    "postcss-calc": "^7.0.1",
    "postcss-custom-properties": "^8.0.9",
    "postcss-discard-duplicates": "^4.0.2",
    "postcss-easings": "^2.0.0",
    "postcss-extend": "^1.0.5",
    "postcss-flexbugs-fixes": "^4.1.0",
    "postcss-import": "^12.0.1",
    "postcss-load-config": "^2.0.0",
    "postcss-loader": "^3.0.0",
    "postcss-mixins": "^6.2.1",
    "postcss-nested": "^4.1.1",
    "postcss-preset-env": "^6.5.0",
    "postcss-reporter": "^6.0.1",
    "postcss-simple-vars": "^5.0.1",
    "postcss-sorting": "^4.0.1",
    "prettier": "^1.16.3",
    "pug": "^2.0.3",
    "pug-lint": "^2.5.0",
    "puglint-stylish": "^0.1.1",
    "react-dev-utils": "^7.0.1",
    "redux-logger": "^3.0.6",
    "rimraf": "^2.6.3",
    "shelljs": "^0.8.3",
    "style-loader": "^0.23.1",
    "stylelint": "^9.10.1",
    "stylelint-config-standard": "^18.2.0",
    "ts-loader": "^5.3.3",
    "tsconfig-paths-webpack-plugin": "^3.2.0",
    "tslint": "^5.12.1",
    "tslint-config-prettier": "^1.17.0",
    "tslint-plugin-prettier": "^2.0.1",
    "tslint-react": "^3.6.0",
    "typescript": "^3.3.1",
    "uglify-save-license": "^0.4.1",
    "uglifyjs-webpack-plugin": "^2.1.1",
    "webpack": "^4.29.0",
    "webpack-dev-middleware": "^3.5.1",
    "webpack-hot-middleware": "^2.24.3"
  },
  "dependencies": {
    "@types/detect-it": "^2.1.0",
    "@types/modernizr": "^3.5.2",
    "@types/ua-parser-js": "^0.7.32",
    "connected-react-router": "^6.2.2",
    "detect-it": "^3.0.5",
    "history": "^4.7.2",
    "immutable": "^4.0.0-rc.12",
    "modernizr": "^3.6.0",
    "react": "^16.7.0",
    "react-dom": "^16.7.0",
    "react-hot-loader": "^4.6.5",
    "react-redux": "^6.0.0",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1",
    "redux": "^4.0.1",
    "redux-thunk": "^2.3.0",
    "reselect": "^4.0.0",
    "ua-parser-js": "^0.7.19"
  }
}
