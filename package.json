{
  "name": "web-template",
  "version": "1.0.0",
  "description": "web development.",
  "repository": {
    "type": "git",
    "url": "git@github.com:kajiyan/web-boilerplate.git"
  },
  "author": "Hiroki Kajiya",
  "browserslist": [
    ">0.25%",
    "ie 11",
    "not op_mini all",
    "not Opera 55"
  ],
  "engines": {
    "npm": ">=6",
    "node": ">=10"
  },
  "license": "MIT",
  "private": true,
  "scripts": {
    "local:dev:server": "NODE_ENV=development node server",
    "local:dev:template": "NODE_ENV=development node internals/pug",
    "global:dev:template": "NODE_ENV=development node internals/pug --location=global",
    "global:pro:template": "NODE_ENV=production node internals/pug",
    "global:dev:scripts": "NODE_ENV=development node internals/webpack --location=global",
    "global:pro:scripts": "NODE_ENV=production node internals/webpack",
    "local:dev:styles": "NODE_ENV=development node internals/postcss",
    "global:dev:styles": "NODE_ENV=development node internals/postcss --location=global",
    "global:pro:styles": "NODE_ENV=production node internals/postcss",
    "lint:staged": "lint-staged",
    "lint:styles": "stylelint \"**/*.pcss\"",
    "lint:styles:fix": "stylelint --fix",
    "lint:scripts": "eslint \"app\" --ext .js,.jsx,tsx,.ts --config .eslintrc.js --ignore-path .gitignore --ignore-pattern internals",
    "lint:scripts:fix": "eslint \"app\" --ext .js,.jsx,tsx,.ts --config .eslintrc.js --ignore-path .gitignore --ignore-pattern internals --fix",
    "dev": "run-p local:dev:*",
    "flow": "flow",
    "flow-typed": "rimraf flow-typed/npm && flow-typed install --overwrite || true"
  },
  "lint-staged": {
    "*.pcss": [
      "yarn run lint:styles:fix",
      "git add --force"
    ],
    "*.ts": [
      "yarn run lint:scripts:fix",
      "git add --force"
    ],
    "ignore": [
      "@types"
    ]
  },
  "pre-commit": "lint:staged",
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@babel/preset-env": "^7.3.1",
    "@babel/preset-flow": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "@babel/register": "^7.0.0",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.5",
    "babel-plugin-flow-runtime": "^0.18.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-flow-strip-types": "^6.22.0",
    "browser-sync": "^2.26.3",
    "browserslist": "^4.4.1",
    "chalk": "^2.4.2",
    "chokidar": "^2.0.4",
    "circular-dependency-plugin": "^5.0.2",
    "config": "^3.0.1",
    "eslint": "^5.12.1",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^4.0.0",
    "eslint-loader": "^2.1.1",
    "eslint-plugin-compat": "^2.6.3",
    "eslint-plugin-flowtype": "^3.2.1",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-jsx-a11y": "^6.2.0",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-react": "^7.12.4",
    "flow-bin": "^0.91.0",
    "flow-runtime": "^0.17.0",
    "flow-typed": "^2.5.1",
    "glob": "^7.1.3",
    "mkdirp": "^0.5.1",
    "moment": "^2.24.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^7.0.14",
    "postcss-load-config": "^2.0.0",
    "prettier": "^1.16.3",
    "pug": "^2.0.3",
    "pug-lint": "^2.5.0",
    "puglint-stylish": "^0.1.1",
    "redux-logger": "^3.0.6",
    "rimraf": "^2.6.3",
    "shelljs": "^0.8.3",
    "stylelint": "^9.10.1",
    "uglify-save-license": "^0.4.1",
    "uglifyjs-webpack-plugin": "^2.1.1",
    "webpack": "^4.29.0",
    "webpack-dev-middleware": "^3.5.1",
    "webpack-hot-middleware": "^2.24.3"
  },
  "dependencies": {
    "connected-react-router": "^6.2.2",
    "history": "^4.7.2",
    "immutable": "^4.0.0-rc.12",
    "react": "^16.7.0",
    "react-dom": "^16.7.0",
    "react-hot-loader": "^4.6.5",
    "react-redux": "^6.0.0",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1",
    "redux": "^4.0.1",
    "redux-thunk": "^2.3.0",
    "reselect": "^4.0.0"
  }
}
