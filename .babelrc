{
  "presets": [
    [
      "@babel/preset-env",
      {
        "targets": {
          "browsers": [
            "last 2 versions",
            "safari >= 7"
          ]
        }
      }
    ],
    "@babel/preset-flow",
    "@babel/preset-react"
  ],
  "plugins": [
    "transform-class-properties",
    [
      "flow-runtime",
      {
        "assert": true,
        "annotate": true
      }
    ]
  ]
}
